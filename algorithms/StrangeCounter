class StrangeCounter {
	static int strangeCounter(long t) {
		int count = 4;
		int gcount = 3;
		int i = 1;
		int ans = 0;
		while (i <= t) {
			count--;
			i++;
			ans = count;
			if (count == 1) {
				gcount = gcount * 2;
				count = gcount + 1;
			}

		}
		return ans;
	}

	public static void main(String args[]) {

		System.out.println(strangeCounter(4));
	}
}
